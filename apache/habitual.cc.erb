<VirtualHost *:80>
    ServerName habitual.cc
    ServerAlias www.habitual.cc
    DocumentRoot /var/www/habitual.cc/current/public
    ErrorLog /var/www/habitual.cc/logs/error.log
    CustomLog /var/www/habitual.cc/logs/access.log combined

    <Directory "/var/www/habitual.cc/current/public">
        Options All
        AllowOverride All
        Order allow,deny
        Allow from all
    </Directory>

    #RewriteEngine On
    # Remove the www
    #RewriteCond %{HTTP_HOST} ^www.habitual.cc$ [NC]
    #RewriteRule ^(.*)$ http://habitual.cc/$1 [R=301,L]
</VirtualHost>
